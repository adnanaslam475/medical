{
  "name": "health_insurance",
  "version": "0.1.0",
  "private": true,
  "main": "dist/server/index.js",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "preserve": "npm run build",
    "serve": "cross-env NODE_ENV=production firebase serve --only functions,hosting",
    "start": "next start",
    "lint": "next lint",
    "build": "next build && next export",
    "dev:client": "next src/client",
    "build:client": "next build src/client",
    "build:server": "babel src/server --out-dir dist/server --source-maps",
    "dev:server": "babel src/server --out-dir dist/server --source-maps --watch",
    "dev": "npm run dev:client & npm run dev:server",
    "predeploy": "rimraf dist/ && npm run build",
    "deploy": "cross-env NODE_ENV=production firebase deploy --only functions,hosting"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@hookform/resolvers": "^2.9.10",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.4",
    "@mui/styled-engine": "^5.11.0",
    "@mui/styles": "^5.11.2",
    "@next/font": "13.1.1",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "axios": "^1.2.1",
    "eslint": "8.31.0",
    "eslint-config-next": "13.1.1",
    "firebase-admin": "^11.5.0",
    "firebase-functions": "^4.2.0",
    "graphql": "^16.6.0",
    "isomorphic-unfetch": "^4.0.2",
    "next": "13.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.42.1",
    "react-select": "^5.7.0",
    "typescript": "4.9.4",
    "urql": "^3.0.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.21",
    "rimraf": "^4.1.2",
    "sass": "^1.57.1",
    "tailwindcss": "^3.2.4"
  }
}
